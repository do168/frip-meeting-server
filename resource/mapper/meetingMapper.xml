<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="meetingMapper">  
  <select id="getMeeting" parameterType="long">
    SELECT
      *
    FROM
      meeting
    WHERE
      id = #{id}
  </select>

  <select id="getHostMeetings" parameterType="long">
    SELECT
      *
    FROM
      meeting
    WHERE
      hostId = #{hostId}
  </select>
  
</mapper>